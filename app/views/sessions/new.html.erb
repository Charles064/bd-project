<h1>Log In</h1>

<%= form_for :session, url: user_login_path do |f| %>
    <p>
        <%= f.label :username %>
        <%= f.text_field :username %>
    </p>
    <p>
        <%= f.label :password, "Password" %>
        <%= f.password_field :password, autocomplete: "off" %>
    </p>
    <p>
        <%= f.submit "Log In"%>
    </p>
    
<% end %> 